class Solution {
    public int minSubArrayLen(int target, int[] nums) {
        int minlength=Integer.MAX_VALUE;
        int start=0,end=0,sum=0;
        int n = nums.length;
        while(end<n){
            
            sum+=nums[end];
           while(sum>=target){
           minlength = Math.min(minlength,end-start+1);
             sum-=nums[start];
            start++;
           }
           end++;
         }
    if(minlength==Integer.MAX_VALUE)return 0;
     return minlength;
    }
}
   
